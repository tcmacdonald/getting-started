//            Form layout styles               //
// ------------------------------------------- //

form {

}

.form-list {
  margin: 0;
  padding: 0;
  
  > li {
    margin: 0 0 calc-em(16px, $base-font-size);
    padding: 0 0 calc-em(4px, $base-font-size);
    list-style-type: none;
  }
  
  .short-clear,
  .full-clear,
  .form-header,
  .form-buttons {
    @include clearfix;
  }
  
  .full-clear,
  .form-header {
    width: 100%;
    input {
      width: 100%;
    }
  }
  
  .form-buttons {
    width: 100%;
    .primary-action {
      @include submit-button;
    }
    .secondary-action {
      @include button-defaults;
    }
    .successful-action {
      @include successful-button;
    }
    .warning-action {
      @include warning-button;
    }
  }
  
  .short-clear,
  .short-right {
    @include column(11.5, last);
    input {
      width: 100%;
    }
  }
  
  .short-right {
    float: right;
  }
  
  label {
    display: block;
    font-weight: bold;
    font-size: calc-em(13px, $base-font-size);
  } //label
  
  .form-title {
    h2 {
      margin: 0;
    }
    em {
      font-size: calc-em(12px, $base-font-size);
    }
  }
  
  .form-header {
    border-bottom: 1px solid #CCC;
    h3 {
      font-size: calc-em(16px, $base-font-size);
      margin: 0;
    }
  }


} //form-list

